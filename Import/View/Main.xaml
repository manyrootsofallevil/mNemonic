<Window x:Class="Import.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="mNeme Manager" Height="300" Width="300" ResizeMode="CanMinimize" Icon="/resources/Manager.ico">
    <!--Icon from here http://bogo-d.deviantart.com/-->
    
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5*"/>
            <ColumnDefinition Width="140*"/>
            <ColumnDefinition Width="5*"/>
        </Grid.ColumnDefinitions>
        <Button Content="Add" HorizontalAlignment="Left" Margin="6,10,0,0" VerticalAlignment="Top" Width="250" Click="Import_Click" Grid.Column="1"/>
        <Button Content="Export" HorizontalAlignment="Left" Margin="6,50,0,0" VerticalAlignment="Top" Width="250" Click="Export_Click" Grid.Column="1"/>
        <Button Content="Import" HorizontalAlignment="Left" Margin="6,90,0,0" VerticalAlignment="Top" Width="250" Click="Button_Click" Grid.Column="1"/>
        <Button Content="Configure Collections" HorizontalAlignment="Left" Margin="6,130,0,0" VerticalAlignment="Top" Width="250" Click="Configure_Click" Grid.Column="1"/>
        <Button Content="Configure mNemonic" HorizontalAlignment="Left" Margin="6,170,0,0" VerticalAlignment="Top" Width="250" Click="ConfiguremNemonic_Click" Grid.Column="1"/>
    </Grid>
</Window>
